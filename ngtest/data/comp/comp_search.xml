<?xml version="1.0" encoding="UTF-8"?>
<TestSuite>
	<TestCase name='single_conds'>
		{conds: [{id: 2, dataType: "270", cval: "天下"}], pageNo: "1", pageSize: "20"}
	</TestCase>
	
	<!-- 多查询条件，带下拉列表 -->
	<TestCase name='multi_conds'>
		{"conds":[{id: 2, dataType: "270", cval: "天下"}, {id: "5", dataType: "200", cval: "北京市"}],"pageNo":"1","pageSize":"20"}
	</TestCase>	
	
	<!-- 无结果 -->	
	<TestCase name='no_result'>
		{"conds":[{id: 2, dataType: "200", cval: "天下"}, {id: "5", dataType: "200", cval: "北京市"}],"pageNo":"1","pageSize":"20"}
	</TestCase>	
	
	<!-- 无查询条件 -->
	<TestCase name='no_conds'>
		{conds: [], pageNo: "1", pageSize: "20"}
	</TestCase>	

	<!-- 数字-等于 -->
	<TestCase name='number_conds'>
		{conds: [{id: "19", dataType: "100", cval: "5"}], pageNo: "1", pageSize: "20"}
	</TestCase>	

	<!-- 日期-大于等于 -->
	<TestCase name='date_conds'>
		{conds: [{id: "6", dataType: "330", cval: "2016-09-07"}], pageNo: "1", pageSize: "20"}
	</TestCase>	
													
</TestSuite>